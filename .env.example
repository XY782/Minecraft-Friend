BOT_ALLOW_CHEATS=false
BOT_LOCAL_LLM_DETECTOR_ENABLED=false
BOT_ENCHANT_SEARCH_RADIUS=20
# Minecraft server connection
MC_HOST=localhost
MC_PORT=25565
MC_USERNAME=MyFriend
# Optional: only if the server requires auth
# MC_PASSWORD=
# MC_AUTH=microsoft

# Version selection
# By default Mineflayer auto-negotiates the version with the server.
# If you want to FORCE a specific version, enable the toggle below.
MC_PIN_VERSION=false
# Example: 1.20.4 (only used when MC_PIN_VERSION=true)
# MC_VERSION=1.20.4

# Gemini (Google Generative AI)
GEMINI_API_KEY=AI
# Examples: gemini-1.5-flash, gemini-1.5-pro
GEMINI_MODEL=

# Bot behavior
# If true, bot will announce it's a bot on join
BOT_ANNOUNCE=true
# Max chat messages per 10 seconds
BOT_CHAT_RATE=5
BOT_REPLY_MODE=nearby
BOT_REPLY_COOLDOWN_MS=2500
BOT_REPLY_DISTANCE=0
BOT_ANTI_REPEAT_ENABLED=true
BOT_ANTI_REPEAT_WINDOW=8

# Autonomous exploring
# Start roaming automatically after spawn
BOT_AUTO_EXPLORE=true
# Random roam target radius in blocks
BOT_EXPLORE_RADIUS=48
# Delay between explore legs in milliseconds
BOT_EXPLORE_PAUSE_MS=5000

# Human-like behavior tuning
BOT_HUMANIZE=true
BOT_MAX_CONCURRENT_ACTIONS=3
# Creative flight rollout (gradual)
BOT_ALLOW_CREATIVE_FLIGHT=true
BOT_CREATIVE_FLIGHT_STAGE=2
# Chat reply delay range in milliseconds
BOT_REPLY_MIN_MS=180
BOT_REPLY_MAX_MS=900
# How often to do idle look-around checks
BOT_LOOK_AROUND_MS=8000

# Training recorder
BOT_TRAINING_ENABLED=true
BOT_TRAINING_INTERVAL_MS=250
BOT_TRAINING_TARGET_FPS=4
BOT_TRAINING_LIVE_CONSOLE=true
BOT_TRAINING_ADAPTIVE_INTERVAL=false
BOT_TRAINING_MIN_INTERVAL_MS=250
BOT_TRAINING_MAX_INTERVAL_MS=250
BOT_TRAINING_DEDUPLICATE_FRAMES=false
BOT_TRAINING_FORCE_RECORD_MS=250


BOT_OBSERVER_MODE=true
BOT_OBSERVER_USERNAME=44_3
BOT_OBSERVER_CAPTURE_RADIUS=0
BOT_OBSERVER_SAMPLE_MIN_MS=250
BOT_OBSERVER_IDLE_SAMPLE_MIN_MS=250
BOT_OBSERVER_FOLLOW_ENABLED=true
BOT_OBSERVER_FOLLOW_DISTANCE=3.5
BOT_OBSERVER_FOLLOW_REFRESH_MS=700

# Optional: use external user telemetry to override observer/bot fields in state-action logs
BOT_USER_TELEMETRY_ENABLED=true
BOT_USER_TELEMETRY_FILE=C:/Users/folder/folder/Folder/minecraft/Training/datasets/latest-user-telemetry.json
BOT_USER_TELEMETRY_MAX_AGE_MS=2000
